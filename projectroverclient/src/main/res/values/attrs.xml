<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TrackpadView">
        <attr name="dimensions" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="both" value="2"/>
        </attr>
        <attr name="toolRadius" format="dimension"/>
        <attr name="trackpadColor" format="color"/>
        <attr name="toolOutlineColor" format="color"/>
        <attr name="toolInsideColor" format="color"/>
        <attr name="minimumChange" format="float"/>
        <attr name="requiredAspectRatio" format="float"/>
    </declare-styleable>
</resources>